    "ng-mission-control": {
      "tags": ["type:ngLib", "scope:shared"]
    },
    "mission-control-core": {
      "tags": ["type:ngLib", "scope:shared"]
    },
    "ng-material": {
      "tags": ["type:ngLib", "scope:shared"]
    },
    "ng-auth": {
      "tags": ["type:ngLib", "scope:shared"]
    },
    "shared": {
      "tags": ["type:ngLib", "scope:shared"]
    }
  }
